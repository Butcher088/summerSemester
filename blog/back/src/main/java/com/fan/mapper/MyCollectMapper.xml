<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.fan.mapper.MyCollectMapper">

    <select id="selctCollectByUserId" resultType="Collect">
        SELECT * FROM collect WHERE user_id=#{userId} AND status=1
    </select>

</mapper>
